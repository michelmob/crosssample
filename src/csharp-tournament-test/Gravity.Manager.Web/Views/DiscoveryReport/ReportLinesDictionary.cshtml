@model IEnumerable<Gravity.Manager.Web.Models.DiscoveryReport.ReportLineViewModel>

<table class="table table-striped table-hover">
    <thead>
    <th>Name</th>
    <th>Value</th>
    </thead>
    <tbody>
    @foreach (var row in Model)
    {
        <tr>
            <td>@row.Name</td>
            <td>@row.Value</td>
        </tr>
    }
</table>


